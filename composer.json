{
    "name": "teners/laravel-link-preview",
    "description": "A Laravel package for extracting link previews with customizable parsers, and caching support",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Teners-net/laravel-link-preview#readme",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "symfony/dom-crawler": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.21"
    },
    "autoload": {
        "psr-4": {
            "Teners\\LaravelLinkPreview\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teners\\LaravelLinkPreview\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Emmanuel Adesina",
            "email": "emmanuel@teners.net",
            "homepage": "https://teners.net",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Teners\\LaravelLinkPreview\\LaravelLinkPreviewServiceProvider"
            ]
        }
    },
    "keywords": [
        "teners",
        "laravel",
        "link preview",
        "metadata",
        "parsing",
        "web scraping",
        "caching",
        "HTML parser",
        "link information",
        "content preview",
        "link extractor",
        "php",
        "laravel package"
    ],
    "scripts": {
        "test": "phpunit"
    }
}
